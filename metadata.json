{
  "name": "adamcrews-pound",
  "version": "2.1.2-rc0",
  "source": "https://github.com/adamcrews/puppet-pound",
  "author": "Adam Crews",
  "license": "Apache-2.0",
  "summary": "Pound Module",
  "description": "Pound Module to manage the Pound SSL/TLS proxy.",
  "project_page": "https://github.com/adamcrews/puppet-pound",
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.7.0 < 5.0.0"
    }
  ],
  "data_provider": "hiera",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.1 < 5.0.0"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 4.0.0 < 5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ]
